[core]
        excludesfile = ~/.gitignore_global
[user]
        name = Jarle Fosen
        email = jarlefosen@gmail.com
[push]
        default = current
[alias]
        d = diff
        l = log --pretty=format:'%h : %ar : %an : %s'
        g = log --graph --decorate --pretty=oneline --abbrev-commit --all
        o = log --oneline
[pull]
	default = simple
