[core]
        excludesfile = ~/.gitignore_global
[user]
        name = Jarle Fosen
        email = jarlefosen@gmail.com
[push]
        default = current
[pager]
        branch = false
[alias]
        d = diff
        b = branch -v
        l = log --pretty=format:'%h : %ar : %an : %s'
        g = log --graph --decorate --pretty=oneline --abbrev-commit --all
        o = log --oneline
	fop = fetch origin --prune
	co = checkout
