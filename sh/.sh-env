#!/bin/sh

# Java
export JAVA_HOME_7="/Library/Java/JavaVirtualMachines/jdk1.7.0_80.jdk/Contents/Home"
export JAVA_HOME_8="/Library/Java/JavaVirtualMachines/adoptopenjdk-8.jdk/Contents/Home/"
export JAVA_HOME_12="/Library/Java/JavaVirtualMachines/openjdk-12.0.1.jdk/Contents/Home"
export JAVA_HOME=$JAVA_HOME_8

# Android
export ANDROID_HOME="$HOME/Library/Android/sdk"
export PATH=$PATH:$ANDROID_HOME/tools

# Go
export GO111MODULE=auto
export GOPATH="$HOME/go"
export PATH="$GOPATH/bin:$PATH"

# Python 3
export PATH="/usr/local/opt/python/libexec/bin:$PATH"

# Yarn
export YARNPATH="$HOME/.yarn"
export PATH=$PATH:$YARNPATH/bin

# Gcloud
export GCLOUD_SDK_HOME="/usr/local/Caskroom/google-cloud-sdk/latest/google-cloud-sdk"
export PATH=$PATH:$GCLOUD_SDK_HOME"/bin"

# Misc
export SCRAPPY_SCRIPTS_SRC="$HOME/dev/go/src/github.com/unacast/scrappy-scripts/src"
export SCRAPPY_SCRIPTS_BIN="$HOME/dev/unacast/scrappy-scripts/bin"
export JARLE_SCRIPTS="$HOME/dev/dotfiles/src"
export VSCODE_BIN="/Applications/Visual Studio Code.app/Contents/Resources/app/bin"
export PATH=$PATH:$SCRAPPY_SCRIPTS_SRC:$SCRAPPY_SCRIPTS_BIN:$JARLE_SCRIPTS:$VSCODE_BIN


export CDPATH=".:$HOME/dev/src/:$GOPATH/src/"
